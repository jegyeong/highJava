<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="kr.or.ddit.student.main.StudentBarChartController">
   <center>
      <BarChart fx:id="bcStudent" prefHeight="300.0" prefWidth="400.0" BorderPane.alignment="CENTER">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
   </center>
   <bottom>
      <Button fx:id="btnClose" mnemonicParsing="false" onAction="#btnCloseClicked" text="닫  기" BorderPane.alignment="CENTER" />
   </bottom>
   <padding>
      <Insets bottom="20.0" top="5.0" />
   </padding>
</BorderPane>
